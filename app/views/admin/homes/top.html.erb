<h2>管理者トップページ</h2>
<h4>会員一覧</h4>
<% @users.each do |user| %>
 <%= link_to admin_user_path(user) do %>
  
   <%= user.id %>
   <%= user.name %>
  
 <% end %>
<% end %>

<h4>相談一覧</h4>
<% @counselings.each do |counseling| %>
 <%= link_to admin_counseling_path(counseling) do %>
 <p>
  <%= counseling.id %>
  <%= counseling.title %>
 </p>
 <% end %>
<% end %>

<h4>回答一覧</h4>
<% @answers.each do |answer| %>
 <%= link_to admin_answer_path(answer) do %>
 <p>
  <%= answer.id %>
  <%= answer.content %>
 </p>
 <% end %>
<% end %>


